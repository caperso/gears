{
    "extends": "react-app",
    "plugins": ["prettier","react-hooks"],
    "rules": {
      "react-hooks/rules-of-hooks":"error",
      "prettier/prettier": "error",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": ["error", {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }]
    }
}
