{
  "name": "gearware",
  "scripts": {
    "push:master": "git checkout master && git merge change-dumi && git push && git checkout change-dumi",
    "prettier": "prettier --write **/*.{js,jsx,tsx,ts,less,md,json}",
    "start": "dumi dev",
    "clean": "rm -rf es/ lib/",
    "build": "dumi build",
    "build:types": "tsc --emitDeclarationOnly",
    "build:package": "npm run clean && npm run build:types && gulp"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
  },
  "devDependencies": {
    "lint-staged": "^10.0.7",
    "yorkie": "^2.0.0",
    "prettier": "^1.19.1",
    "dumi": "1.0.13",
    "antd": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "through2": "^3.0.1",
    "typescript": "^3.8.3",
    "vfile-message": "^2.0.3"
  },
  "version": "0.1.2",
  "license": "MIT"
}
