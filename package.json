{
  "name": "gearware",
  "version": "0.1.18",
  "description": "react component, ui lib, like image-viewer",
  "keywords": [
    "react",
    "component",
    "ui",
    "rc",
    "image",
    "viewer"
  ],
  "author": "caperso",
  "scripts": {
    "push:master": "git checkout master && git merge change-dumi && git push && git checkout change-dumi",
    "prettier": "prettier --write **/*.{js,jsx,tsx,ts,less,md,json}",
    "start": "dumi dev",
    "clean": "rm -rf esm lib",
    "build": "dumi build",
    "build:types": "tsc --emitDeclarationOnly",
    "build:package": "npm run clean && npm run build:types && gulp"
  },
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "antd": "^4.1.0",
    "dumi": "1.0.13",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "through2": "^3.0.1",
    "typescript": "^3.8.3",
    "vfile-message": "^2.0.3",
    "yorkie": "^2.0.0"
  },
  "files": [
    "lib",
    "esm"
  ],
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
